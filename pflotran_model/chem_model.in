# Description: 1D MORB alteration simulation

SIMULATION
  SIMULATION_TYPE SUBSURFACE
  PROCESS_MODELS
    SUBSURFACE_FLOW flow
      MODE TH
    /
    SUBSURFACE_TRANSPORT transport
      MODE GIRT
    /
  /
  RESTART
    FILENAME partial_equil.chk
    RESET_TO_TIME_ZERO
  /
  CHECKPOINT
  /
END

OVERWRITE_RESTART_TRANSPORT 

SUBSURFACE

#=========================== numerical methods ================================
NUMERICAL_METHODS FLOW
  NEWTON_SOLVER
  /
END

NUMERICAL_METHODS TRANSPORT
  NEWTON_SOLVER
  /
END



# =========================== discretization ===================================
GRID
  TYPE structured
  ORIGIN 0.d0 0.d0 0.d0
  NXYZ 128 1 128
  BOUNDS
    0    0  0
    1500  100  1500
  /
END


#=========================== fluid properties =================================
FLUID_PROPERTY
  DIFFUSION_COEFFICIENT 1.d-9
END


#=========================== Equation of state =================================
EOS WATER
  DENSITY IF97
  ENTHALPY IF97
END

# =========================== material properties ==============================
MATERIAL_PROPERTY sheeted_dykes
  ID 1
  POROSITY 0.01
  TORTUOSITY 1.0d0
  PERMEABILITY
    PERM_ISO 2e-15
  /
  CHARACTERISTIC_CURVES cc1
  THERMAL_CONDUCTIVITY_DRY 1.82
  THERMAL_CONDUCTIVITY_WET 1.82
  SPECIFIC_HEAT 800
  ROCK_DENSITY 2700
END

MATERIAL_PROPERTY pillows
  ID 2
  POROSITY 0.01
  TORTUOSITY 1.0d0
  PERMEABILITY
    PERM_ISO 200.d-15
  /
  CHARACTERISTIC_CURVES cc1
  THERMAL_CONDUCTIVITY_DRY 1.82
  THERMAL_CONDUCTIVITY_WET 1.82
  SPECIFIC_HEAT 800
  ROCK_DENSITY 2700
END


MATERIAL_PROPERTY altered_pillows
  ID 3
  POROSITY 0.01
  TORTUOSITY 1.0d0
  PERMEABILITY
    PERM_ISO 20.d-15
  /
  CHARACTERISTIC_CURVES cc1
  THERMAL_CONDUCTIVITY_DRY 1.82
  THERMAL_CONDUCTIVITY_WET 1.82
  SPECIFIC_HEAT 800
  ROCK_DENSITY 2700
END

MATERIAL_PROPERTY low_perm
  ID 4
  POROSITY 0.01
  TORTUOSITY 1.0d0
  PERMEABILITY
    PERM_ISO 0.2d-15
  /
  CHARACTERISTIC_CURVES cc1
  THERMAL_CONDUCTIVITY_DRY 1.82
  THERMAL_CONDUCTIVITY_WET 1.82
  SPECIFIC_HEAT 800
  ROCK_DENSITY 2700
END


#=========================== characteristic curves ============================
CHARACTERISTIC_CURVES cc1
  skip
  SATURATION_FUNCTION VAN_GENUCHTEN
    LIQUID_RESIDUAL_SATURATION 0.1d0
    M 0.8d0
    ALPHA 1.d-4
  /
  PERMEABILITY_FUNCTION MUALEM_VG_LIQ
    LIQUID_RESIDUAL_SATURATION 0.1d0
    M 0.8d0
  /
  noskip
  DEFAULT
END


# =========================== output options ====================================
OUTPUT
  SNAPSHOT_FILE
    FORMAT HDF5 
    PERIODIC TIME 50 y
    VARIABLES
      TEMPERATURE
      LIQUID_PRESSURE
      LIQUID_DENSITY
      LIQUID_SATURATION
      LIQUID_VISCOSITY
      PERMEABILITY
      POROSITY
    /
  /
  VELOCITY_AT_CENTER
END


# =========================== times ==================================
TIME
  FINAL_TIME 7000 y
  MAXIMUM_TIMESTEP_SIZE 5000 y
  #INITIAL_TIMESTEP_SIZE 1d-16 y
END

# =========================== regions ==================================
REGION all
  COORDINATES
    0.d0 0.d0 0.d0
    1500 1.d2 1500
  /
END

REGION downwelling
  COORDINATES
    175  0.d0 1090
    1500 1.d2 1500
  /
END

REGION upwelling
  COORDINATES
    0    0.d0 1090
    60   1.d2 1500
  /
END


REGION low_perm
  COORDINATES
    60   0.d0 1090
    175  1.d2 1500
  /
END


# =========================== boundaries ==================================
REGION reaction_zone
  COORDINATES
    0.d0  0.d0  0.d0
    1500  1.d2  1.d0
  /
  FACE BOTTOM
END


REGION seafloor
  COORDINATES
    0.00d0 0.d0  1499
    1500   1.d2  1500
  /
  FACE TOP
END

REGION seafloor_in
  COORDINATES
    175    0.d0  1499
    1500   1.d2  1500
  /
  FACE TOP
END

REGION seafloor_out
  COORDINATES
    0.00  0.d0  1499
    175   1.d2  1500
  /
  FACE TOP
END



# =========================== flow conditions ==================================
FLOW_CONDITION initial
  TYPE
    LIQUID_PRESSURE HYDROSTATIC
    TEMPERATURE DIRICHLET
  /
  DATUM 0 0.d0 1500.d0
  LIQUID_PRESSURE 3.d7
  TEMPERATURE 2
  GRADIENT 
     TEMPERATURE -6.67d-4 0 -0.2
  /
END

FLOW_CONDITION seafloor_out
  TYPE
    LIQUID_PRESSURE DIRICHLET 
    ENERGY_FLUX NEUMANN
  /
  LIQUID_PRESSURE 3.d7
  ENERGY_FLUX 0.0 W/m^2
END

FLOW_CONDITION seafloor_in
  TYPE
    LIQUID_PRESSURE DIRICHLET 
    TEMPERATURE DIRICHLET
  /
  LIQUID_PRESSURE 3.d7
  TEMPERATURE 2.d0
END

FLOW_CONDITION magma_lens
  TYPE
    LIQUID_FLUX NEUMANN
    TEMPERATURE DIRICHLET
  /
  TEMPERATURE 350.d0
  LIQUID_FLUX 0.d0
END


# =========================== condition couplers ===============================
# initial condition
INITIAL_CONDITION
  FLOW_CONDITION initial
  TRANSPORT_CONDITION initial
  REGION all
END

# top boundary condition
BOUNDARY_CONDITION
  FLOW_CONDITION seafloor_in
  TRANSPORT_CONDITION seafloor
  REGION seafloor_in
END

BOUNDARY_CONDITION
  FLOW_CONDITION seafloor_out
  TRANSPORT_CONDITION empty
  REGION seafloor_out
END

# bottom boundary condition
BOUNDARY_CONDITION
  FLOW_CONDITION magma_lens
  TRANSPORT_CONDITION empty
  REGION reaction_zone
END


#=========================== stratigraphy couplers ============================
STRATA
  REGION all
  MATERIAL sheeted_dykes
END

STRATA
  REGION upwelling
  MATERIAL altered_pillows
END

STRATA
  REGION downwelling
  MATERIAL pillows
END

STRATA
  REGION low_perm
  MATERIAL low_perm
END

